@model NomiPro.ModelDB.HORARIO

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalles de horarios</h2>

<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
           <label><b>Tipo de horario</b></label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tipo_Horario)
        </dd>

        <dt>
          <label><b>Horario laboral</b></label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Hora_Trabajo)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.ID_Horario }, new { @class = "btn btn-outline-warning" })
    @Html.ActionLink("Borrar", "Delete", new { id = Model.ID_Horario}, new { @class = "btn btn-outline-danger waves-effect" })
    <a href="~/HORARIOs/index" class="btn btn-primary waves-effect">Volver atras</a>
  
</p>
