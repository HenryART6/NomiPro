@model NomiPro.ModelDB.NOMINA

@{
    
    ViewBag.Title = "Iniciar sesión";
    Layout = null;
}

<!DOCTYPE html>

<html sstyle="background-color:#2F3E47">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="author" content="Coderthemes">

    <link rel="shortcut icon" href="~/Nomicon.png">

    <title>Nomipro</title>

    <!-- App css -->

    @Styles.Render("~/Content/css")
    @Styles.Render("~/bundles/modernizr")


</head>


<body>
    <div class="card-body, rounded">
        <img src="~/Nomicon.png" alt="user-img" title="Este es nuestro logo :3" class="rounded-circle img-responsive"> 
        <a href="~/Nomicon.png" class="logo"><span>Nomi<span>Pro</span></span><i class="mdi mdi-layers "></i></a>

        <p class="card-body rounded">N O M I N A</p>
        <hr />
        <dl class="card-body" style="background-color:#2F3E47">
            <dt>
                <label><b>MES</b></label>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Mes)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Estado)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Estado)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Subtotal)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Subtotal)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Total)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Total)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.CONTROL_PAGO.Mes)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CONTROL_PAGO.Mes)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.EMPLEADOS.Nombre)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.EMPLEADOS.Nombre)
            </dd>

        </dl>
        2019 NomiPro.
    </div>







    <!-- jQuery  -->
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/popper.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/detect.js"></script>
    <script src="~/Scripts/fastclick.js"></script>
    <script src="~/Scripts/jquery.blockUI.js"></script>
    <script src="~/Scripts/waves.js"></script>
    <script src="~/Scripts/jquery.nicescroll.js"></script>
    <script src="~/Scripts/jquery.slimscroll.js"></script>
    <script src="~/Scripts/jquery.scrollTo.min.js"></script>

    <!-- App js -->
    <script src="~/Scripts/jquery.core.js"></script>
    <script src="~/Scripts/jquery.app.js"></script>


</body>
</html>
