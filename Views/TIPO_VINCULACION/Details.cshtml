@model NomiPro.ModelDB.TIPO_VINCULACION

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalles de tipo de vinculación</h2>

<div>
   
    <hr />
    <dl class="dl-horizontal">
        <dt>
           <label><b>Descripción</b></label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Descripcion)
        </dd>

        <dt>
            <label><b>Estado</b></label>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Estado)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.ID_Vinculacion }, new { @class = "btn btn-outline-warning" })
    @Html.ActionLink("Borrar", "Delete", new { id = Model.ID_Vinculacion }, new { @class = "btn btn-outline-danger waves-effect" })
    <a href="~/TIPO_VINCULACION/index" class="btn btn-primary waves-effect">Volver atras</a>
   
</p>
