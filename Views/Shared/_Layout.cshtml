<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="author" content="Coderthemes">

    <link rel="shortcut icon" href="~/Nomicon.png">

    <title>Nomipro</title>

    <!-- App css -->

    @Styles.Render("~/Content/css")
    @Styles.Render("~/bundles/modernizr")

</head>


<body class="fixed-left">

    <!-- Begin page -->
    <div id="wrapper ">

        <!-- Top Bar Start -->
        <div class="topbar ">

            <!-- LOGO -->
            <div class="topbar-left">
                <img src="~/favicon.jpg" alt="user-img" title="Este es nuestro logo :3" class="rounded-circle img-responsive" width="50" height="50">
                <a href="~/Home/index" class="logo"><span>Nomi<span>Pro</span></span><i class="mdi mdi-layers waves-effect"></i></a>

            </div>

            <!-- Button mobile view to collapse sidebar menu -->

            <div class="navbar navbar-default" role="navigation">
                <div class="container-fluid">

                    <!-- Page title -->
                    <ul class="nav navbar-nav list-inline navbar-left">
                        <li class="list-inline-item">
                            <button class="button-menu-mobile open-left">
                                <i class="mdi mdi-menu"></i>
                            </button>
                </div><!-- end container -->
            </div><!-- end navbar -->


        </div>

        </div>
        <!-- Top Bar End -->
        <!-- ========== Left Sidebar Start ========== -->
        <div class="left side-menu ">
            <div class="sidebar-inner slimscrollleft">
                <div>
                    <h5 style="margin-left:60px">@Html.Partial("_LoginPartial")</h5>
                </div>
               
                <div id="sidebar-menu">
                    <ul>
                        <li style="text-align:center" class=" text-muted menu-title">N O M I N A</li>

                        <li>
                            <a href="~/NOMINAs/index" class="waves-effect"><i class="fa  fa-diamond"></i> <span> Nomina </span> </a>
                        </li>

                        <li>
                            <a href="~/EMPLEADOS/index" class="waves-effect"><i class="fa fa-group"></i> <span> Empleados </span> </a>
                        </li>

                        <li>
                            <a href="~/CONTROL_PAGO/index" class="waves-effect"><i class="fa fa-credit-card-alt"></i> <span> Control de Pagos </span> </a>
                        </li>

                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect"><i class="fa  fa-suitcase"></i> <span>Gestión de Cargos </span> <span class="menu-arrow"></span></a>
                            <ul class="list-unstyled">
                                <li><a href="~/CARGOS/index" class=" fa fa-black-tie waves-effect"><span style="margin-left:15px">Cargos</span></a></li>
                                <li><a href="~/TIPO_VINCULACION/index" class="fa fa-link waves-effect"><span style="margin-left:15px">Tipo de Vinculación</span></a></li>

                            </ul>
                        </li>

                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect"><i class="fa fa-hourglass-3"></i><span>Gestión de Horarios </span> <span class="menu-arrow"></span></a>

                            <ul class="list-unstyled">
                                <li><a href="~/HORARIOs/index" class="fa fa-calendar waves-effect"><span style="margin-left:15px">Horarios</span></a></li>
                                <li><a href="~/HORA_EXTRAS/index" class="fa fa-clock-o waves-effect"><span style="margin-left:15px">Horas Extras</span></a></li>
                                <li><a href="~/HETRAXEMPLEADOes/index" class="fa fa-plus-square-o waves-effect"><span style="margin-left:15px">Extras por Empleado</span></a></li>

                            </ul>
                        </li>

                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect"><i class=" fa fa-industry waves-effect"></i> <span> Gestión Parafiscales </span> <span class="list-unstyled"></span><span class="menu-arrow"></span></a>
                            <ul class="list-unstyled">
                                <li><a href="~/PARAFISCALES/index" class="fa fa-legal waves-effect"><span style="margin-left:15px">Parafiscales</span></a></li>
                                <li><a href="~/PARAXEMPLEs/index" class="fa fa-list waves-effect"><span style="margin-left:15px">Parafiscales por Empleados</span></a></li>

                            </ul>
                        </li>


                    </ul>
                    <div class="clearfix"></div>
                </div>
                <!-- Sidebar -->
                <div class="clearfix"></div>

            </div>

        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class="container-fluid">
                        @RenderBody()

                </div> <!-- container -->

            </div> <!-- content -->

            <footer class="footer text-right">
                2016 - 2019 © NomiPro.
            </footer>

        </div>


        <!-- ============================================================== -->
        <!-- End Right content here -->
        <!-- ============================================================== -->
        <!-- Right Sidebar -->
        
        <!-- /Right-bar -->

    </div>
    <!-- END wrapper -->
    <!-- jQuery  -->
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/popper.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/detect.js"></script>
    <script src="~/Scripts/fastclick.js"></script>
    <script src="~/Scripts/jquery.blockUI.js"></script>
    <script src="~/Scripts/waves.js"></script>
    <script src="~/Scripts/jquery.nicescroll.js"></script>
    <script src="~/Scripts/jquery.slimscroll.js"></script>
    <script src="~/Scripts/jquery.scrollTo.min.js"></script>

    <!-- App js -->
    <script src="~/Scripts/jquery.core.js"></script>
    <script src="~/Scripts/jquery.app.js"></script>

</body>
</html>
