@model IEnumerable<NomiPro.ModelDB.PARAXEMPLE>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Parafiscales por empleado</h2>

<p>
    <a href="/PARAXEMPLEs/Create" class="btn btn-success"> Crear nuevo </a>
</p>
<table class="table">
    <tr>
        <th>
            <label><b>Mes</b></label>
        </th>
        <th>
            <label><b>Nombre de empleado</b></label>
        </th>
        <th>
            <label><b>Nombre de parafiscales</b></label>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Mes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EMPLEADOS.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PARAFISCALES.Nombre)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ID_PAEM }, new { @class = "btn btn-outline-primary waves-effect" })
            @Html.ActionLink("Detalles", "Details", new { id=item.ID_PAEM }, new { @class = "btn btn-outline-warning waves-effect" })
            @Html.ActionLink("Eliminar", "Delete", new { id=item.ID_PAEM }, new { @class = "btn btn-outline-danger waves-effect" })
        </td>
    </tr>
}

</table>
